<?xml version="1.0" encoding="utf-8" ?>
<core:BasePage
    x:Class="MAUIsland.ResumesTemplate.ResumesPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:app="clr-namespace:MAUIsland"
    xmlns:core="clr-namespace:MAUIsland.Core;assembly=MAUIsland.Core"
    xmlns:local="clr-namespace:MAUIsland.ResumesTemplate"
    Title="ResumesPage"
    x:DataType="local:ResumesPageViewModel">
    <VerticalStackLayout>
        <Button
            Command="{x:Binding NavigateUrlCommand}"
            CommandParameter="{x:Static app:AppRoutes.ResumeDetailPage}"
            Text="Navigate to detail" />
    </VerticalStackLayout>
</core:BasePage>